@model Contact
@{
    ViewBag.Title = "Create Contact";
}

<br />

<div class="well">
    <form class="form-horizontal" method="post">
        <div class="form-group xrm-header">
            <div class="col-sm-2 xrm-header-label">Create Contact</div>
            <div class="col-sm-2">
                <button type="submit" class="btn btn-primary">Save</button>
                <a class="btn btn-primary" href="/Home">Cancel</a>
            </div>
        </div>

        @Html.EditorForModel()

    </form>
</div>